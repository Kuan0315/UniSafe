{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    },
    "skipLibCheck": true      // <--- ignore type errors in node_modules
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "services/guardianService.js",
   "app/(tabs)/TrustedContactsScreen"  ],
  "exclude": ["node_modules"]
}